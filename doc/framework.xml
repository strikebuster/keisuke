  <keisuke:Framework name="Struts" group="Struts">
    <keisuke:SpecificType name="HTML" group="Web">
      <keisuke:PathPattern>.*/webapp/.*\.html?$</keisuke:PathPattern>
    </keisuke:SpecificType>
    <keisuke:SpecificType name="CSS" group="Web">
    	<keisuke:PathPattern>.*/webapp/.*\.css$</keisuke:PathPattern>
    </keisuke:SpecificType>
    <keisuke:SpecificType name="JavaScript" group="Web">
    	<keisuke:PathPattern>.*/webapp/.*\.js$</keisuke:PathPattern>
    </keisuke:SpecificType>
    <keisuke:SpecificType name="JSP" group="Struts">
    	<keisuke:PathPattern>.*/webapp/.*\.jsp$</keisuke:PathPattern>
    </keisuke:SpecificType>
    <keisuke:SpecificType name="ConfigXML" group="Struts">
    	<keisuke:PathPattern>.*/WEB-INF/.*/struts-config\.xml$</keisuke:PathPattern>
    </keisuke:SpecificType>
    <keisuke:SpecificType name="ValidationXML" group="Struts">
    	<keisuke:PathPattern>.*/WEB-INF/.*/.*valid.*\.xml$</keisuke:PathPattern>
    </keisuke:SpecificType>
    <keisuke:SpecificType name="ActFormBeanJava" group="Struts">
    	<keisuke:PathPattern>.*/webapp/.*/.*Bean\.java$</keisuke:PathPattern>
    </keisuke:SpecificType>
    <keisuke:SpecificType name="ActionJava" group="Struts">
    	<keisuke:PathPattern>.*/webapp/.*/.*Action\.java$</keisuke:PathPattern>
    </keisuke:SpecificType>
    <keisuke:SpecificType name="Java" group="Struts">
    	<keisuke:PathPattern>.*/java/.*/.*\.java$</keisuke:PathPattern>
    </keisuke:SpecificType>
  </keisuke:Framework>

