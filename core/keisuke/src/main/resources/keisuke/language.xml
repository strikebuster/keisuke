			<keisuke:LineComment>
				<keisuke:start>#</keisuke:start>
			</keisuke:LineComment>
			<keisuke:BlockComment>
				<keisuke:start>&apos;&apos;&apos;</keisuke:start>
				<keisuke:end>&apos;&apos;&apos;</keisuke:end>
			</keisuke:BlockComment>
			<keisuke:BlockComment>
